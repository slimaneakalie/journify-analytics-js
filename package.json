{
  "name": "@journifyio/analytics",
  "version": "0.0.68",
  "description": "Journify analytics SDK in JavaScript",
  "main": "dist/lib/es5/index.js",
  "types": "dist/lib/es5/index.d.ts",
  "module": "dist/lib/es6/index.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": "git@github.com:slimaneakalie/journify-analytics-js.git",
  "homepage": "https://github.com/slimaneakalie/journify-analytics-js",
  "author": "Slimane AkaliÃ«",
  "license": "MIT",
  "keywords": [
    "journify",
    "analytics"
  ],
  "scripts": {
    "setup": "cp scripts/githooks/* .git/hooks/ && chmod +x .git/hooks/*",
    "build": "npm run setup && npm run clean && npm run update-version && npm run lint && tsc && tsc -m es6 --outDir dist/lib/es6 && webpack",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "update-version": "sh scripts/update-version-var.sh",
    "major-version": "npm --no-git-tag-version version major && npm run update-version && git commit -am 'update version to major'",
    "minor-version": "npm --no-git-tag-version version minor && npm run update-version && git commit -am 'update version to minor'",
    "patch-version": "npm --no-git-tag-version version patch && npm run update-version && git commit -am 'update version to patch'"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.6.1",
    "@types/spark-md5": "^3.0.2",
    "buffer": "^6.0.3",
    "js-cookie": "^3.0.1",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
